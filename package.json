    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "babel-eslint": "^9.0.0",
